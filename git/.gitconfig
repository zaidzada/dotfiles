# .gitconfig
#
# Git configuration file

[alias]
	a = add
	b = branch -a
	c = commit
	d = diff
	l = log --pretty=oneline -n 20 --graph --abbrev-commit
	r = remote -v
	s = status -s
	t = tag -l

	cm = commit -m
	co = checkout
	st = status

[core]
	editor = vim

[help]
	autocorrect = 1

[push]
	default = simple

[user]
	name = Zaid Kokaja

[github]
	user = zkokaja
