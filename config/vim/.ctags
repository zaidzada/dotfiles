--langdef=markdown
--langmap=markdown:.mkd
--regex-markdown=/^#[ \t]+(.*)/\1/h,header1/
--regex-markdown=/^##[ \t]+(.*)/\1/i,header2/
--regex-markdown=/^###[ \t]+(.*)/\1/k,header3/
--regex-make=/^([^# \t]*):/\1/t,target/
--regex-make=/^(#+ >) ?(.*)/> \2/h,header,Makefile Header/
--regex-sh=/^(#+ >) ?(.*)/> \2/h,header,Shell Header/
--regex-python=/^(#+ >) ?(.*)/> \2/h,header,Python Header/
